{
  "name": "bGone",
  "short_name": "bGone",
  "version": "1.2",
  "description": "Click elements to delete them from the page",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "icons": {
    "16": "icons/Icon-16.png",
    "32": "icons/Icon-32.png",
    "48": "icons/Icon-48.png",
    "96": "icons/Icon-transparent.svg"
  },
  "manifest_version": 2
}